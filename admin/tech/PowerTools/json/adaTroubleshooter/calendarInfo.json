[
  ~[tlist_sql;
SELECT to_char(a.date_value, '~[datetext:mmddyyyy]'), a.insession, a.membershipvalue,
a.a, a.b, a.c, a.d, a.e, a.f,
b.name, c.day_name, d.ada_value_code, d.ada_value_time, d.adm_value, e.att_code, e.calculate_ada_yn,
e.calculate_adm_yn, e.description, e.presence_status_cd
FROM ps.calendar_day a
LEFT JOIN ps.bell_schedule b
ON a.bell_schedule_id = b.id
LEFT JOIN ps.cycle_day c
ON a.cycle_day_id = c.id
LEFT JOIN ps.attendance d
ON a.id = d.calendar_dayid AND d.studentid = ~(curstudid) AND d.att_mode_code='ATT_ModeDaily'
LEFT JOIN ps.attendance_code e
ON d.attendance_codeid = e.id
WHERE a.date_value >= to_date(~[gpv:startDate],'MM/DD/YYYY') AND a.date_value <= to_date(~[gpv:endDate],'MM/DD/YYYY')AND a.schoolid='100'
ORDER BY a.date_value
]
{
  "date": "~(a.date_value)",
  "inSession": "~(a.insession)",
  "membershipValue": "~(a.membershipvalue)",
  "tracks": ["~(a.a)", "~(a.b)", "~(a.c)", "~(a.d)", "~(a.e)", "~(a.f)"],
  "bellScheduleName": "~(b.name)",
  "cycleDayName": "~(c.day_name)",
  "adaValueCode": "~(d.ada_value_code)",
  "adaValueTime": "~(d.ada_value_time)",
  "admValue": "~(d.adm_value)",
  "attCode": "~(e.att_code)",
  "countsInADA": "~(e.calculate_ada_yn)",
  "countsInAdm": "~(e.calculate_adm_yn)",
  "codeDescription": "~(e.description)",
  "presenceStatus": "~(e.presence_status_cd)"
},
[/tlist_sql]
{}
]
