[
~[tlist_sql;
SELECT to_char(entrydate,'~[datetext:mmddyyyy]'), to_char(exitdate,'~[datetext:mmddyyyy]'), grade_level
FROM ps.reenrollments
WHERE studentid=~(curstudid) AND schoolid=~(curschoolid)
ORDER BY entrydate DESC
]
{
"entryDate": "~(entrydate)",
"exitDate": "~(exitdate)",
"gradeLevel": "~(grade_level)"
},
[/tlist_sql]
{}
]



