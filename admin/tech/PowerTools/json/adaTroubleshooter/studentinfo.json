[
~[tlist_sql;
SELECT to_char(a.entrydate, '~[datetext:mmddyyyy]'), to_char(a.exitdate, '~[datetext:mmddyyyy]'), a.membershipshare, a.track, a.grade_level, a.fteid, a.schoolid,
        b.name, b.yearid, b.dflt_att_mode_code, b.dflt_conversion_mode_code
FROM (
SELECT entrydate, exitdate, membershipshare, track, grade_level, fteid, schoolid
FROM ps.students
WHERE id=~(curstudid) AND schoolid=~(curschoolid)
UNION ALL
SELECT entrydate, exitdate, membershipshare, track, grade_level, fteid, schoolid
FROM ps.reenrollments
WHERE studentid=~(curstudid) AND schoolid=~(curschoolid)
) a
LEFT JOIN ps.fte b
ON b.id = a.fteid
ORDER BY entrydate DESC
]
{
"entryDate": "~(a.entrydate)",
"exitDate": "~(a.exitdate)",
"membershipShare": "~(a.membershipshare)",
"track": "~(a.track)",
"gradeLevel": "~(a.grade_level)",
"fteId": "~(a.fteid)",
"schoolid": "~(a.schoolid)",
"fteName" : "~(b.name)",
"yearid" : "~(b.yearid)",
"attModeCode" : "~(b.dflt_att_mode_code)",
"conversionModeCode" : "~(b.dflt_conversion_mode_code)"
},
[/tlist_sql]
{}
]